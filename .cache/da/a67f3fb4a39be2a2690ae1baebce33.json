l":{"line":100,"column":4},"generated":{"line":1,"column":2207}},{"source":"src/CategoryPages/Men/displayCarouselManPage.js","name":"widgets","original":{"line":100,"column":18},"generated":{"line":1,"column":2209}},{"source":"src/CategoryPages/Men/displayCarouselManPage.js","name":"widgets","original":{"line":100,"column":18},"generated":{"line":1,"column":2217}},{"source":"src/CategoryPages/Men/displayCarouselManPage.js","name":"pagination","original":{"line":100,"column":26},"generated":{"line":1,"column":2225}},{"source":"src/CategoryPages/Men/displayCarouselManPage.js","original":{"line":100,"column":37},"generated":{"line":1,"column":2236}},{"source":"src/CategoryPages/Men/displayCarouselManPage.js","name":"container","original":{"line":101,"column":6},"generated":{"line":1,"column":2237}},{"source":"src/CategoryPages/Men/displayCarouselManPage.js","original":{"line":101,"column":17},"generated":{"line":1,"column":2247}},{"source":"src/CategoryPages/Men/displayCarouselManPage.js","name":"search","original":{"line":105,"column":2},"generated":{"line":1,"column":2265}},{"source":"src/CategoryPages/Men/displayCarouselManPage.js","name":"start","original":{"line":105,"column":9},"generated":{"line":1,"column":2267}},{"source":"src/CategoryPages/Men/displayCarouselManPage.js","original":{"line":106,"column":1},"generated":{"line":1,"column":2275}},{"source":"src/CategoryPages/Men/displayCarouselManPage.js","original":{"line":106,"column":1},"generated":{"line":1,"column":2283}},{"source":"src/CategoryPages/Men/displayCarouselManPage.js","original":{"line":106,"column":1},"generated":{"line":1,"column":2292}}],"sources":{"src/CategoryPages/Men/displayCarouselManPage.js":"import { connectHits } from 'instantsearch.js/es/connectors';\n// import { configure, hits, EXPERIMENTAL_configureRelatedItems } from 'instantsearch.js/es/widgets';\nimport instantsearch from 'instantsearch.js';\nimport algoliasearch from 'algoliasearch';\n\nexport const carousel = connectHits(function renderCarousel(\n  { widgetParams: { container, title }, hits },\n  isFirstRender\n) {\n  if (isFirstRender) {\n    console.log(container);\n    const section = document.createElement('section');\n    section.classList.add('section-carousel-winter');\n    container.appendChild(section);\n    const divSection = document.createElement('div')\n    divSection.classList.add('title-carousel-winter')\n    section.appendChild(divSection)\n    const titleSection = document.createElement('h2')\n    titleSection.innerText = title\n    divSection.appendChild(titleSection)\n    const btnSection = document.createElement('a')\n    btnSection.classList.add('btn-carousel-winter')\n    btnSection.innerHTML = \"See All\"\n    btnSection.href = \"./index.html\"\n    divSection.appendChild(btnSection)\n\n\n    // document\n    //   .querySelector('.title-carousel-winter').insertAdjacentHTML(\n    //     'afterbegin',\n    //     `<h2>${title}</h2><a href=\"/searchResults.html\" class=\"btn-carousel-winter\">See All</a>`\n    //   );\n\n\n    const ul = document.createElement('ul');\n    ul.classList.add('carousel-list-container');\n    section.appendChild(ul);\n  }\n\n  container.querySelector('ul').innerHTML = hits\n    .map(\n      hit => `\n        <li>\n          <div class=\"image-wrapper\">\n            <img src=\"${hit.image_link}\" alt=\"${hit.name}\">\n          </div>\n          <div class=\"info\">\n            <h3 class=\"title\">${hit.name}</h3>\n          </div>\n        </li>\n      `\n    )\n    .join('');\n});\n\n\nexport function renderCarouselAllProduct() {\n\n  const searchClient = algoliasearch('HYDY1KWTWB', '28cf6d38411215e2eef188e635216508');\n\n  const search = instantsearch({\n    indexName: 'gstar_demo_test',\n    searchClient,\n  });\n\n  search.addWidgets([\n    instantsearch.widgets.searchBox({\n      container: '#searchbox',\n      placeholder: 'Clothes, Sneakers among men category',\n    }),\n    instantsearch.widgets.configure({\n      hitsPerPage: 15,\n      ruleContexts: ['man_page'],\n      filters: '\"hierarchical_categories.lvl0\":\"men\"'\n    }),\n    instantsearch.widgets.stats({\n      container: '#stats-searchResultMan',\n    }),\n    instantsearch.widgets.hits({\n      container: '#hits',\n      templates: {\n        item: `\n \n      <div class=\"image-wrapper\">\n          <img src=\"{{image_link}}\" align=\"left\" alt=\"{{name}}\" class=\"result-img\" />\n      </div>\n      <div class=\"hit-name\">\n          <div class=\"hit-infos\">\n         \n          <div>{{#helpers.highlight}}{ \"attribute\": \"name\" }{{/helpers.highlight}}</div>\n              <div class=\"hit-colors\">{{colourFilter}}</div>\n          </div>\n          <div class=\"hit-price\">\\${{price}}</div>\n      </div>\n      \n\n  `,\n      },\n    }),\n    instantsearch.widgets.pagination({\n      container: '#pagination',\n    }),\n  ]);\n\n  search.start()\n};"},"lineCount":null}},"error":null,"hash":"553817138302eff034fa8542f48da714","cacheData":{"env":{}}}