"line":200,"column":0}},{"source":"src/SearchResult_JS/css/searchBoxResults.css","original":{"line":201,"column":0},"generated":{"line":201,"column":0}},{"source":"src/SearchResult_JS/css/searchBoxResults.css","original":{"line":202,"column":0},"generated":{"line":202,"column":0}},{"source":"src/SearchResult_JS/css/searchBoxResults.css","original":{"line":203,"column":0},"generated":{"line":203,"column":0}},{"source":"src/SearchResult_JS/css/searchBoxResults.css","original":{"line":204,"column":0},"generated":{"line":204,"column":0}},{"source":"src/SearchResult_JS/css/searchBoxResults.css","original":{"line":205,"column":0},"generated":{"line":205,"column":0}},{"source":"src/SearchResult_JS/css/searchBoxResults.css","original":{"line":206,"column":0},"generated":{"line":206,"column":0}},{"source":"src/SearchResult_JS/css/searchBoxResults.css","original":{"line":207,"column":0},"generated":{"line":207,"column":0}},{"source":"src/SearchResult_JS/css/searchBoxResults.css","original":{"line":208,"column":0},"generated":{"line":208,"column":0}}],"sources":{"src/SearchResult_JS/css/searchBoxResults.css":"\n\n.ais-Hits-list{\nwidth: 90vw;\nmargin: 2rem auto;\ndisplay: grid;\ngrid-template-columns: repeat(3, 1fr);\nmargin: 1rem auto;\njustify-content: center;\nalign-items: center;\n}\n.image-wrapper{\n    display: flex;\n    position: relative;\n}\n\n.result-img{\n    width: 50%;\n    margin: 0 auto;\n    justify-self: center;\n    margin-bottom: 3rem;\n    filter: drop-shadow(rgb(240, 240, 239) 32px 60px 20px);\n    transition: 0.2s ease-in-out;\n}\n\n.result-img:hover {\n    transform: scale(1.1);\n    filter: drop-shadow(16px 16px 20px rgb(240,240,239));\n    transition: 0.2s ease-in-out;\n    }\n\n  \n.ais-Hits-item{\n    height: 100%;\n    padding: 1.5rem;\n    margin: 1rem 0;\n    width: 100%;\n    background-color: white;\n    position: relative;\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    border: 1px solid rgba(240,240,239, 0.9);\n    box-shadow: none;\n    justify-self: center;\n    transition: 0.2s ease-in-out;\n}\n\n.ais-Hits-item:hover{  \n  box-shadow: 4px 4px 20px rgba(240, 240, 239, 1) ;\n  transition: 0.2s ease-in-out;\n}\n\n.hit-name {\n    color: black;\n    font-family: 'Roboto';\n    font-weight: bold;\n    font-size: 1rem;\n    margin: 1rem 0;\n    display: flex;\n    \n}\n\n.hit-infos{\n    display: flex;\n    justify-content: space-around;\n    width: 100%;\n    flex-wrap: wrap;\n}\n\n.hit-price{\n    position: absolute;\n    bottom: 1rem;\n    right: 1rem;\n}\n\n.hit-description{\n    color: black;\n    font-family: 'Roboto';\n    font-weight: 100;\n    font-style: italic;\n    font-size: 0.8rem;\n    margin: 1rem auto;\n}\n\n\n.hit-colors{\n    font-size: 0.8rem;\n    font-style: italic;\n}\n\n.ais-Highlight-highlighted{\n    background-color: rgb(240, 240, 239);\n}\n\n/* COLOR CODE */\n\n.colorWrapper{\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    align-items: center;\n}\n\n.hit-colorsHex{\n    width: 20px;\n    height: 20px;\n    border-radius: 50%;\n    border:1px solid rgba(240,240,239, 0.9);\n    margin-top: 1rem;\n}\n\n\n/* SIZE FILTER HOVER */\n\n.hit-sizeFilter{\n    visibility: hidden;\n    opacity: 0;\n    display: flex;\n    justify-content: center;\n    position: absolute;\n    bottom: 0;\n    left: 0;\n    align-items: center;\n    width: 100%;\n    height: 4rem;\n    background-color: rgba(255, 255, 255, 0.8);\n    color: black;\n}\n.hit-sizeFilter p{\n    /* visibility: hidden; */\n   font-size: 0.8rem;\n   font-family: 'Roboto';\n    color: black;\n    font-weight: bold;\n    \n}\n.hit-sizeFilter span{\n    /* visibility: hidden; */\n   font-size: 0.8rem;\n   font-family: 'Roboto';\n    color: black;\n    font-weight: medium;\n    font-style: italic;\n    \n}\n\n.fadeInSize{\n    animation: fadeInSize 0.5s forwards;\n}\n.fadeOutSize{\n    animation: fadeOutSize 0.1s forwards;\n}\n\n@media screen and (max-width: 768px){\n    body {\n        overflow-x: hidden !important;\n    }\n    .ais-Hits-list{\n        grid-template-columns: repeat(2, 1fr);\n    }\n   }\n   @media screen and (max-width: 560px){\n    body {\n      overflow-x: hidden !important;\n  }\n  .ais-Hits-list{\n    grid-template-columns: repeat(1, 1fr);\n}\n\n.ais-Stats-text{\n    font-size: 0.8rem;\n}\n.btn-filter {\n    padding: 0.2rem 1rem;\n}\n}\n\n\n/* ANIMATION SIZE HOVER */\n\n@keyframes fadeInSize{\n    0%{\n        visibility: hidden;\n        transform: translateY(4rem) scale(1);\n        opacity: 0;\n\n\n    }\n    100%{\n        visibility: visible;\n        opacity: 1;\n        transform: translateY(0rem) scale(1.1);\n    }\n}\n@keyframes fadeOutSize{\n    0%{\n        visibility: visible;\n        opacity: 1;\n        transform: translateY(0rem) scale(1.1);\n    }\n    100%{\n        visibility: hidden;\n        opacity: 0;\n        transform: translateY(4rem) scale(1);\n\n    }\n}"},"lineCount":208}},"error":null,"hash":"7451d4266b91e61fde3a484797a682a3","cacheData":{"env":{}}}