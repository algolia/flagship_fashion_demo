mily: \"Roboto\";\n  color: black;\n  align-self: center;\n  cursor: pointer;\n  transition: 0.3s ease-in-out;\n}\n.btn-carousel-winter:hover{\n  background-color: rgb(17, 17, 17);\n  color: white;\n  transition: 0.3s ease-in-out;\n}\n\n/* HORIZONTAL SCROLLBAR */\n\n.carousel .carousel-list-container::-webkit-scrollbar {\n  -webkit-appearance: none;\n}\n\n.carousel .carousel-list-container::-webkit-scrollbar:horizontal {\n  height: 4px;\n\n}\n\n.carousel .carousel-list-container::-webkit-scrollbar-thumb {\n  border-radius: 8px;\n  border: 1px solid white;\n  background-color: rgba(0, 0, 0, .9);\n  \n}\n.carousel .carousel-list-container::-webkit-scrollbar-track { \n  background-color: #fff; \n  border-radius: 8px;\n  \n}\n\n.carousel .carousel-list-container li {\n  min-height: 450px;\n  padding: 1.5rem;\n  margin: auto 1rem;\n  width: 380px;\n  background-color: white;\n  position: relative;\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  border: 1px solid rgba(240,240,239, 0.9);\n  transition: 0.2s ease-in-out;\n\n}\n.carousel .carousel-list-container li:hover {\n\n  box-shadow: 4px 4px 20px rgba(240, 240, 239, 1) ;\n  transition: 0.2s ease-in-out;\n}\n\n\n.carousel .carousel-list-container li .title {\ntext-align: center;\nfont-family: 'Roboto';\nfont-weight: 500;\nfont-size: 0.8rem;\n\n}\n\n.carousel .carousel-list-container .image-wrapper {\n max-width: 90%;\n display: flex;\n justify-content: center;\n margin: 0 auto;\npadding: 1rem;\n \n}\n\n.carousel .carousel-list-container li img {\n  width: 70%;\n  object-fit: contain;\n  align-self: center;\n  margin-top: -6rem;\n  margin-bottom: 1rem;\n  filter: drop-shadow(rgb(240, 240, 239) 32px 60px 20px);\n  transition: 0.2s ease-in-out;\n}\n.carousel .carousel-list-container li img:hover {\ntransform: scale(1.1);\nfilter: drop-shadow(16px 16px 20px rgb(240,240,239));\ntransition: 0.2s ease-in-out;\n}\n\n/* STAT */\n.ais-Stats{\n  font-family: \"Roboto\" ;\n  font-weight: 100;\n  font-style: italic;\n  color: black;\n  float: right;\n}\n\n/* USER PERSONA SELECTOR */\n\n.labelUserPersonna{\n  position: fixed;\n  top: 20vh;\n  left: -1rem;\n  width: 40px;\n  height: 100px;\n  background-color: white;\n  border-radius: 5px;\n  border: 1px solid rgba(240,240,239, 0.5);\n  z-index: 16;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  animation: bounce 2s infinite;\n  cursor: pointer;\n}\n.labelUserPersonna p{\n  writing-mode: vertical-rl;\n  margin-left: 20px;\n  text-orientation: sideways-right;\n  font-size: 0.8rem;\n  font-family: 'Castoro', serif;\n  font-style: italic;\n}\n\n.userPersonaSelector {\n visibility: hidden;\n display: flex;\n flex-direction: column;\n justify-content: center;\n background-color: white;\n color: black;\n width: 200px;\n padding: 1rem;\n text-align: center;\n box-shadow: 4px 4px 20px rgb(0, 0, 0, 0.3);\n border-radius: 3px;\n position: fixed;\n top: 20vh;\n left: 0;\n z-index: 15;\n}\n\n.closePersona{\ncursor: pointer;\nposition: absolute;\ntop: 0.1rem;\nright: 0.8rem;\n}\n\n.userPersonaSelector p {\n  color: black;\n  font-size: 0.8rem;\n  font-style: italic;\n  font-family: 'Castoro', serif;\n}\n.userPersonaSelector select {\n  background-color:rgb(246,245,243);\n     border: none;\n     padding: 0.6rem;\n     padding-left: 3rem;\n     border-radius: 5px;\n     font-family: 'Castoro', serif;\n}\n\n.personaFadeIn{\n  animation: personaIn 0.8s forwards;\n}\n.personaFadeOut{\n  animation: personaOut 0.8s forwards;\n}\n\n\n@keyframes personaIn{\n  0%{\n    visibility: hidden;\n    transform: translateX(-100%);\n  }\n  100%{\n    visibility: visible;\n    transform: translateX(0%);\n  }\n}\n@keyframes personaOut{\n  0%{\n    visibility: visible;\n    transform: translateX(0%);\n  }\n  100%{\n    visibility: hidden;\n    transform: translateX(-100%);\n  }\n}\n@keyframes bounce{\n  0%{\n    transform: translateX(0) scale(1);\n  }\n  50%{\n    transform: translateX(5px);\n  }\n  75%{\n    transform: translateX(3px);\n  }\n  80%{\n    transform: translateX(2px);\n  }\n  90%{\n    transform: translateX(4px);\n  }\n  100%{\n    transform: translateX(0px);\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"eed676d8d410cf94b7a9894e084f5275","cacheData":{"env":{}}}