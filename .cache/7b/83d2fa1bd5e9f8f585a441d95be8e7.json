n":25},"generated":{"line":1,"column":2698}},{"source":"src/SearchResult_JS/relatedResultModal.js","name":"add","original":{"line":58,"column":35},"generated":{"line":1,"column":2708}},{"source":"src/SearchResult_JS/relatedResultModal.js","original":{"line":58,"column":39},"generated":{"line":1,"column":2712}},{"source":"src/SearchResult_JS/relatedResultModal.js","name":"modalWrapper","original":{"line":59,"column":12},"generated":{"line":1,"column":2729}},{"source":"src/SearchResult_JS/relatedResultModal.js","name":"classList","original":{"line":59,"column":25},"generated":{"line":1,"column":2731}},{"source":"src/SearchResult_JS/relatedResultModal.js","name":"remove","original":{"line":59,"column":35},"generated":{"line":1,"column":2741}},{"source":"src/SearchResult_JS/relatedResultModal.js","original":{"line":59,"column":42},"generated":{"line":1,"column":2748}},{"source":"src/SearchResult_JS/relatedResultModal.js","name":"modalWrapper","original":{"line":61,"column":12},"generated":{"line":1,"column":2766}},{"source":"src/SearchResult_JS/relatedResultModal.js","name":"classList","original":{"line":61,"column":25},"generated":{"line":1,"column":2768}},{"source":"src/SearchResult_JS/relatedResultModal.js","name":"add","original":{"line":61,"column":35},"generated":{"line":1,"column":2778}},{"source":"src/SearchResult_JS/relatedResultModal.js","original":{"line":61,"column":39},"generated":{"line":1,"column":2782}},{"source":"src/SearchResult_JS/relatedResultModal.js","name":"modalWrapper","original":{"line":62,"column":12},"generated":{"line":1,"column":2798}},{"source":"src/SearchResult_JS/relatedResultModal.js","name":"classList","original":{"line":62,"column":25},"generated":{"line":1,"column":2800}},{"source":"src/SearchResult_JS/relatedResultModal.js","name":"remove","original":{"line":62,"column":35},"generated":{"line":1,"column":2810}},{"source":"src/SearchResult_JS/relatedResultModal.js","original":{"line":62,"column":42},"generated":{"line":1,"column":2817}},{"source":"src/SearchResult_JS/relatedResultModal.js","name":"getObjectID","original":{"line":51,"column":4},"generated":{"line":1,"column":2840}}],"sources":{"src/SearchResult_JS/relatedResultModal.js":"import algoliasearch from 'algoliasearch';\nimport instantsearch from 'instantsearch.js';\nimport { configure, hits, EXPERIMENTAL_configureRelatedItems } from 'instantsearch.js/es/widgets';\n\n\nexport function relatedResultModal() {\n    const searchClient = algoliasearch('HYDY1KWTWB', '28cf6d38411215e2eef188e635216508');\n    const index = searchClient.initIndex('gstar_demo_test');\n\n\n\n    const search = instantsearch({\n        indexName: 'gstar_demo_test',\n        searchClient\n    });\n\n\n    let searchInput = document.querySelector('.ais-SearchBox-input')\n    searchInput.addEventListener('change', (e) => {\n        if (e) {\n            domListening()\n        }\n    })\n\n    const domListening = () => {\n        // Listen to the Dom and change the content of the relatedsearch carousel with the search\n        const observer = new MutationObserver(mutation => {\n            if (mutation) {\n                console.log(mutation)\n                getObjectID()\n            }\n        });\n        observer.observe(document.body, {\n            childList: true,\n            subtree: true,\n        });\n    }\n\n    const getObjectID = () => {\n        let productSearchResult = document.querySelectorAll('.product-searchResult');\n        productSearchResult.forEach((item) => {\n            item.addEventListener('click', (e) => {\n                e.preventDefault()\n                index.getObject(item.dataset.id).then(object => {\n                    displayrelateditems(object);\n                })\n                displayModal()\n            })\n        })\n    }\n    getObjectID()\n\n\n\n    function displayModal() {\n        let modalWrapper = document.querySelector('.modal-relatedItems--wrapper');\n        if (modalWrapper.classList.contains('displayBlock')) {\n            modalWrapper.classList.add('fadeOutFilter')\n            modalWrapper.classList.remove('displayBlock')\n        } else {\n            modalWrapper.classList.add('displayBlock')\n            modalWrapper.classList.remove('fadeOutFilter')\n        }\n    }\n\n    // Display the related Search carousel according to the product chosen by user\n    function displayrelateditems(object) {\n\n        const referenceHit = {\n            objectID: object.objectID,\n            category: object.category,\n            colors: object.colors,\n            description: object.description,\n            dynamic_attributes: object.dynamic_attributes,\n            genderFilter: object.genderFilter,\n            name: object.name,\n            keywords: object.keywords,\n            default_variant: object.default_variant,\n            image_link: object.image_link,\n            colourFilter: object.colourFilter,\n            hierarchical_categories: object.hierarchical_categories,\n            non_numeric_attributes: object.non_numeric_attributes,\n            price: object.price,\n            priceFilter: object.priceFilter,\n            sizeFilter: object.sizeFilter,\n            position: object.position,\n            url: object.url,\n            numeric_attributes: object.numeric_attributes,\n            fitFilter: object.fitFilter,\n            neckFilter: object.neckFilter,\n            sleeveFilter: object.sleeveFilter,\n            availabilityDetail: object.availabilityDetail,\n            fullStock: object.fullStock,\n            sizes: object.sizes\n        };\n        console.log('referenceHit')\n\n        // Add the widgets\n        search.addWidgets([\n            configure({\n                hitsPerPage: 8,\n                query: '',\n            }),\n            EXPERIMENTAL_configureRelatedItems({\n                hit: referenceHit,\n                matchingPatterns: {\n                    genderFilter: { score: 3 },\n                    category: { score: 2 },\n                },\n            }),\n            hits({\n                container: '#carousel-relatedItems',\n                templates: {\n                    item: (hit) =>\n\n                        `          \n                        <a href=\"${hit.url}\" class=\"product-searchResult\" data-id=\"${hit.objectID}\">\n                        <div class=\"image-wrapper\">\n                            <img src=\"${hit.image_link}\" align=\"left\" alt=\"${hit.name}\" class=\"result-img\" />\n                        </div>\n                        <div class=\"hit-name\">\n                            <div class=\"hit-infos\">\n                            <div>${hit.name}</div>\n                                <div class=\"hit-colors\">${hit.colourFilter}</div>\n                            </div>\n                            <div class=\"hit-price\">$${hit.price}</div>\n                        </div>\n                    </a>\n                        `\n                }\n            }),\n        ]);\n\n        search.start()\n    }\n\n}\n\n"},"lineCount":null}},"error":null,"hash":"996d046f39fab384b67c517b2125c3ee","cacheData":{"env":{}}}