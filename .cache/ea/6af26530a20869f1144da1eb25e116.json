adding: 1.5rem;\n    margin: auto 1rem;\n    width: 380px;\n    background-color: white;\n    position: relative;\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    border: 1px solid rgba(240,240,239, 0.9);\n    transition: 0.2s ease-in-out;\n  \n  }\n  .carousel .carousel-list-container li:hover {\n  \n    box-shadow: 4px 4px 20px rgba(240, 240, 239, 1) ;\n    transition: 0.2s ease-in-out;\n  }\n  \n  \n  .carousel .carousel-list-container li .title {\n  text-align: center;\n  font-family: 'Roboto';\n  font-weight: 500;\n  font-size: 0.8rem;\n  \n  }\n  \n  .carousel .carousel-list-container .image-wrapper {\n   max-width: 90%;\n   display: flex;\n   justify-content: center;\n   margin: 0 auto;\n  padding: 1rem;\n   \n  }\n  \n  .carousel .carousel-list-container li img {\n    width: 70%;\n    object-fit: contain;\n    align-self: center;\n    margin-top: -6rem;\n    margin-bottom: 1rem;\n    filter: drop-shadow(rgb(240, 240, 239) 32px 60px 20px);\n    transition: 0.2s ease-in-out;\n  }\n  .carousel .carousel-list-container li img:hover {\n  transform: scale(1.1);\n  filter: drop-shadow(16px 16px 20px rgb(240,240,239));\n  transition: 0.2s ease-in-out;\n  }\n  \n  /* STAT */\n  .ais-Stats{\n    font-family: \"Roboto\" ;\n    font-weight: 100;\n    font-style: italic;\n    color: black;\n    float: right;\n  }\n\n  /* MAN PAGE RESULT */\n\n\n\n.ais-Hits-list{\n  width: 90vw;\n  margin: 2rem auto;\n  display: grid;\n  grid-template-columns: repeat(3, 1fr);\n  margin: 1rem auto;\n  justify-content: center;\n  align-items: center;\n  }\n  .image-wrapper{\n      display: flex\n  }\n  \n  .result-img{\n      width: 50%;\n      margin: 0 auto;\n      justify-self: center;\n      margin-bottom: 3rem;\n      filter: drop-shadow(rgb(240, 240, 239) 32px 60px 20px);\n      transition: 0.2s ease-in-out;\n  }\n  \n  .result-img:hover {\n      transform: scale(1.1);\n      filter: drop-shadow(16px 16px 20px rgb(240,240,239));\n      transition: 0.2s ease-in-out;\n      }\n  \n    \n  .ais-Hits-item{\n      height: 100%;\n      padding: 1.5rem;\n      margin: 1rem 0;\n      width: 100%;\n      background-color: white;\n      position: relative;\n      display: flex;\n      flex-direction: column;\n      justify-content: center;\n      border: 1px solid rgba(240,240,239, 0.9);\n      box-shadow: none;\n      justify-self: center;\n      transition: 0.2s ease-in-out;\n  }\n  \n  .ais-Hits-item:hover{  \n    box-shadow: 4px 4px 20px rgba(240, 240, 239, 1) ;\n    transition: 0.2s ease-in-out;\n  }\n  \n  .hit-name {\n      color: black;\n      font-family: 'Roboto';\n      font-weight: bold;\n      font-size: 1rem;\n      margin: 1rem 0;\n      display: flex;\n      \n  }\n  \n  .hit-infos{\n      display: flex;\n      justify-content: space-around;\n      width: 100%;\n      flex-wrap: wrap;\n  }\n  \n  .hit-price{\n      position: absolute;\n      bottom: 1rem;\n      right: 1rem;\n  }\n  \n  .hit-description{\n      color: black;\n      font-family: 'Roboto';\n      font-weight: 100;\n      font-style: italic;\n      font-size: 0.8rem;\n      margin: 1rem auto;\n  }\n  \n  \n  .hit-colors{\n      font-size: 0.8rem;\n      font-style: italic;\n  }\n  \n  .ais-Highlight-highlighted{\n      background-color: rgb(240, 240, 239);\n  }\n  \n  @media screen and (max-width: 768px){\n      body {\n          overflow-x: hidden !important;\n      }\n      .ais-Hits-list{\n          grid-template-columns: repeat(2, 1fr);\n      }\n     }\n     @media screen and (max-width: 560px){\n      body {\n        overflow-x: hidden !important;\n    }\n    .ais-Hits-list{\n      grid-template-columns: repeat(1, 1fr);\n  }\n  \n  .ais-Stats-text{\n      font-size: 0.8rem;\n  }\n  .btn-filter {\n      padding: 0.2rem 1rem;\n  }\n  }"},"lineCount":256}},"error":null,"hash":"23630c3dcade8578d17a7fefef5cfcc7","cacheData":{"env":{}}}