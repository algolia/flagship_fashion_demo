  margin: 5px;\n    border-radius: 15px;\n    background-color: black;\n    transition: all 0.4s ease;\n}\n\n.nav-active {\n transform: translateX(0) !important;\n transition: transform 0.5s ease-in-out;\n}\n\n.displayFlex{\n  display: flex !important;\n}\n\n.toggle .line1{\n    transform: rotate(-45deg) translate(-5px, 6px);\n}\n.toggle .line2{\n    opacity: 0;\n}\n.toggle .line3{\n    transform: rotate(45deg) translate(-5px, -6px);\n}\n\n\n\n@media screen and (max-width: 1024px){\n\n  .searchbar-wrapper{\n   width: 100%;\n   justify-self: center;\n    }\n\n  .nav-wrapper{\n      display: grid;\n      grid-template-columns: 25% 1fr 25%;\n      justify-content: center;\n      grid-template-areas: \"logo searchbar burger\";\n      margin: 0 1rem;\n      justify-items: center;\n  }\n  .burger {\n      display: block;\n      cursor: pointer;\n  }\n  .nav-list ul {\n      position: absolute;\n      right: 0;\n      top: 3.6rem;\n      height: 100vh;\n      z-index: 1000;\n      background-color: black;\n      color: white;\n      display: flex;\n      flex-direction: column;\n      align-items: center;\n      width: 35%;\n      transform: translateX(100%);\n      transition: transform 0.5s ease-in-out;\n      list-style: none;\n  }\n  .nav-list ul li a{\n      opacity: 0;\n      color: white; \n  }\n}\n\n @media screen and (max-width: 768px){\n  .burger {\n      display: block;\n  }\n }\n @media screen and (max-width: 560px){\n  body {\n    overflow-x: hidden !important;\n  }\n\n  .header{\n    width: 100vw;\n    padding-bottom: 1rem;   \n  }\n\n  .nav-wrapper{\n    display: grid;\n    grid-template-columns: 25% 1fr 25%;\n    justify-content: center;\n    grid-template-areas: \". logo burger\"\n                          \"searchbar searchbar searchbar\";\n    margin: 0 auto;\n    justify-items: center;\n}\n\n  .searchbar-wrapper{\n    width: 100%;\n    margin: 1rem auto;\n    justify-self: center;\n  }\n\n  .btn-filter{\n    font-size: 0.8rem;\n  }\n\n   .burger div {\n    height: 2px;\n    width: 25px;\n  }\n\n  .ais-Hits-item {\n    height: 450px;\n    padding: 1.5rem;\n    margin: auto 1rem;\n    width: 250px;\n  }\n\n  .nav-list ul {\n  top: 5.6rem;\n  width: 60%;\n  }\n\n  .ais-SearchBox-input::placeholder {\n    font-size: 0.6rem;\n  }\n}  \n\n@media screen and (max-width: 361px){\nbody {\n  overflow-x: hidden !important;\n}\n}\n\n @keyframes navlinksFade {\n     from {\n         display: none;\n         opacity: 0;\n     }\n     to {\n         display: flex;\n         opacity: 1;           \n     }\n }\n @keyframes navLinksMove {\n     from {\n         transform: translateX(100px);\n     }\n     to {\n          transform: translateX(0px);\n     }\n }"},"lineCount":null}},"error":null,"hash":"2c5787d3ad217706b83ec853968775f8","cacheData":{"env":{}}}