":"item","original":{"line":84,"column":16},"generated":{"line":1,"column":1726}},{"source":"../src/CategoryPages/Women/displayCarouselWomanPage.js","original":{"line":84,"column":20},"generated":{"line":1,"column":1731}},{"source":"../src/CategoryPages/Women/displayCarouselWomanPage.js","original":{"line":102,"column":8},"generated":{"line":1,"column":2202}},{"source":"../src/CategoryPages/Women/displayCarouselWomanPage.js","original":{"line":102,"column":19},"generated":{"line":1,"column":2204}},{"source":"../src/CategoryPages/Women/displayCarouselWomanPage.js","original":{"line":102,"column":19},"generated":{"line":1,"column":2206}},{"source":"../src/CategoryPages/Women/displayCarouselWomanPage.js","original":{"line":102,"column":19},"generated":{"line":1,"column":2218}},{"source":"../src/CategoryPages/Women/displayCarouselWomanPage.js","name":"container","original":{"line":103,"column":12},"generated":{"line":1,"column":2219}},{"source":"../src/CategoryPages/Women/displayCarouselWomanPage.js","original":{"line":103,"column":23},"generated":{"line":1,"column":2229}},{"source":"../src/CategoryPages/Women/displayCarouselWomanPage.js","name":"search","original":{"line":107,"column":4},"generated":{"line":1,"column":2247}},{"source":"../src/CategoryPages/Women/displayCarouselWomanPage.js","name":"start","original":{"line":107,"column":11},"generated":{"line":1,"column":2249}},{"source":"../src/CategoryPages/Women/displayCarouselWomanPage.js","original":{"line":108,"column":1},"generated":{"line":1,"column":2257}},{"source":"../src/CategoryPages/Women/displayCarouselWomanPage.js","original":{"line":108,"column":1},"generated":{"line":1,"column":2265}},{"source":"../src/CategoryPages/Women/displayCarouselWomanPage.js","original":{"line":108,"column":1},"generated":{"line":1,"column":2274}}],"sources":{"../src/CategoryPages/Women/displayCarouselWomanPage.js":"import instantsearch from \"instantsearch.js\";\nimport algoliasearch from 'algoliasearch';\nimport { configure, pagination, hits, stats, searchBox } from 'instantsearch.js/es/widgets'\nimport { connectHits } from 'instantsearch.js/es/connectors';\n\n\n\nexport const carousel = connectHits(function renderCarousel(\n    { widgetParams: { container, title }, hits },\n    isFirstRender\n) {\n    if (isFirstRender) {\n        console.log(container);\n        const section = document.createElement('section');\n        section.classList.add('section-carousel-winter');\n        container.appendChild(section);\n        const divSection = document.createElement('div')\n        divSection.classList.add('title-carousel-winter')\n        section.appendChild(divSection)\n        const titleSection = document.createElement('h2')\n        titleSection.innerText = title\n        divSection.appendChild(titleSection)\n        const btnSection = document.createElement('a')\n        btnSection.classList.add('btn-carousel-winter')\n        btnSection.innerHTML = \"See All\"\n        btnSection.href = \"./index.html\"\n        divSection.appendChild(btnSection)\n\n\n        // document\n        //   .querySelector('.title-carousel-winter').insertAdjacentHTML(\n        //     'afterbegin',\n        //     `<h2>${title}</h2><a href=\"/searchResults.html\" class=\"btn-carousel-winter\">See All</a>`\n        //   );\n\n\n        const ul = document.createElement('ul');\n        ul.classList.add('carousel-list-container');\n        section.appendChild(ul);\n    }\n\n    container.querySelector('ul').innerHTML = hits\n        .map(\n            hit => `\n        <li>\n          <div class=\"image-wrapper\">\n            <img src=\"${hit.image_link}\" alt=\"${hit.name}\">\n          </div>\n          <div class=\"info\">\n            <h3 class=\"title\">${hit.name}</h3>\n          </div>\n        </li>\n      `\n        )\n        .join('');\n});\n\n\nexport function renderCarouselAllProduct() {\n\n    const searchClient = algoliasearch('HYDY1KWTWB', '28cf6d38411215e2eef188e635216508');\n\n    const search = instantsearch({\n        indexName: 'gstar_demo_test',\n        searchClient,\n    });\n\n    search.addWidgets([\n        searchBox({\n            container: '#searchbox',\n            placeholder: 'Clothes, Sneakers among women category',\n        }),\n        configure({\n            hitsPerPage: 15,\n            ruleContexts: ['woman_page'],\n            filters: '\"hierarchical_categories.lvl0\":\"women\"'\n        }),\n        stats({\n            container: '#stats-searchResultWoman',\n        }),\n        hits({\n            container: '#hits',\n            templates: {\n                item: `\n \n      <div class=\"image-wrapper\">\n          <img src=\"{{image_link}}\" align=\"left\" alt=\"{{name}}\" class=\"result-img\" />\n      </div>\n      <div class=\"hit-name\">\n          <div class=\"hit-infos\">\n         \n          <div>{{#helpers.highlight}}{ \"attribute\": \"name\" }{{/helpers.highlight}}</div>\n              <div class=\"hit-colors\">{{colourFilter}}</div>\n          </div>\n          <div class=\"hit-price\">\\${{price}}</div>\n      </div>\n      \n\n  `,\n            },\n        }),\n        pagination({\n            container: '#pagination',\n        }),\n    ]);\n\n    search.start()\n};"},"lineCount":null}},"error":null,"hash":"29e8be9106b03b6f34f5771b59826865","cacheData":{"env":{}}}