on: fadeOutModal 0.3s ease-in-out forwards;\n}\n.fadeInModal{\n    animation: fadeInModal 1.3s ease-in-out forwards;\n}\n\n.modal-carousel--wraper{\n    display: flex;\n    flex-direction: column;\n}\n\n.modal-carousel--title{\n    position: relative;\n    width: fit-content;\n}\n.modal-carousel--title h2{\n   margin: 5rem 2rem 1rem 6rem;\n   font-size: 2rem;\n\n}\n\n.underline{\n    border: 1px solid rgba(0, 0, 0, 0);\n    box-shadow: 1px 1px 0px rgba(0, 0,0, 0.1);\n    width: 50%;\n    position: absolute;\n    right: -30px;\n}\n\n.carousel-relatedItems .ais-Hits-list{\n    width: 90%;\n    flex-wrap: nowrap !important;\n    display: -webkit-box !important;\n    overflow-x: scroll !important;\n    padding: 2rem;\n    list-style-type: none;\n}\n.carousel-relatedItems  .ais-Hits-item{\n    height: 450px;\n    padding: 1.5rem;\n    margin: auto auto;\n    width: 380px;\n    background-color: white;\n    position: relative;\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    border: 1px solid rgba(240,240,239, 0.9);\n    box-shadow: none;\n    transition: 0.2s ease-in-out;\n}\n\n\n.carousel-relatedItems .ais-Hits-item:hover{  \n  box-shadow: 4px 4px 20px rgba(240, 240, 239, 1) ;\n  transition: 0.2s ease-in-out;\n}\n.carousel-relatedItemsSecond .ais-Hits-list{\n    width: 90%;\n    flex-wrap: nowrap !important;\n    display: -webkit-box !important;\n    overflow-x: scroll !important;\n    padding: 2rem;\n    list-style-type: none;\n}\n.carousel-relatedItemsSecond  .ais-Hits-item{\n    height: 450px;\n    padding: 1.5rem;\n    margin: auto auto;\n    width: 380px;\n    background-color: white;\n    position: relative;\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    border: 1px solid rgba(240,240,239, 0.9);\n    box-shadow: none;\n    transition: 0.2s ease-in-out;\n}\n\n\n.carousel-relatedItemsSecond .ais-Hits-item:hover{  \n  box-shadow: 4px 4px 20px rgba(240, 240, 239, 1) ;\n  transition: 0.2s ease-in-out;\n}\n\n/* HORIZONTAL SCROLLBAR */\n\n.ais-Hits-list::-webkit-scrollbar {\n    -webkit-appearance: none;\n  }\n  \n.ais-Hits-list::-webkit-scrollbar:horizontal {\n    height: 4px;\n  \n  }\n  \n.ais-Hits-list::-webkit-scrollbar-thumb {\n    border-radius: 8px;\n    border: 1px solid white;\n    background-color: rgba(0, 0, 0, .9);\n    \n  }\n.ais-Hits-list::-webkit-scrollbar-track { \n    background-color: #fff; \n    border-radius: 8px;\n  }\n\n.modal-relatedItems--closeBtn{\n    position: fixed;\n    top: 1rem;\n    right: 1rem;\n    cursor: pointer;\n    padding: 0.5rem;\n    border: 1px solid rgba(8, 8, 8, 0.3);\n}\n\n@keyframes fadeModal {\n    0%{\n        transform: translateY(-100%);\n        visibility: visible;\n        opacity: 0.2;\n    }\n    100%{\n        transform: translateY(0%);\n        visibility: visible;\n        opacity: 1;\n    }\n}\n@keyframes fadeOutModal {\n    0%{\n        transform: translateY(0%);\n        visibility: visible;\n        opacity: 1;\n    }\n    100%{\n        transform: translateY(-100%);\n        visibility: hidden;\n        opacity: 0;\n    }\n}\n@keyframes fadeInModal {\n    0%{\n        /* transform: translateY(0%); */\n        transform: translate(-50%, 100%);\n        visibility: hidden;\n        opacity: 0;\n    }\n    100%{\n        /* transform: translateY(-100%); */\n        transform: translate(-50%, -50%);\n        visibility: visible;\n        opacity: 1;\n    }\n}\n"},"lineCount":null}},"error":null,"hash":"1fd4d58ee73852605541790965849e7e","cacheData":{"env":{}}}