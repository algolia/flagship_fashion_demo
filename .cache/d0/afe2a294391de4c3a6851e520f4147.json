r{\n    padding: 0.6rem 2rem;\n    border: 1px solid rgba(0, 0, 0, .5);\n    font-size: 1;\n    font-family: \"Roboto\";\n    color: black;\n    align-self: center;\n    cursor: pointer;\n    transition: 0.3s ease-in-out;\n}\n\n.btn-filter:hover {\n    background-color: rgb(17, 17, 17);\n    color: white;\n    transition: 0.3s ease-in-out;\n}\n\n.btn-filter i{\n  margin-left: 0.5rem;\n}\n\n.left-panel{\n    display: grid;\n    grid-template-columns: repeat(3, 1fr);\n    justify-content: center;\n    justify-items: center;\n    align-items: flex-start;\n    padding: 2rem;\n}\n.left-panel div{\n    width: 100%;\n    height: 100%;\n    display: flex;\n    flex-direction: column;\n\n}\n\n .category-wrapper, .gender-wrapper{\n    border-right: 1px solid rgba(0, 0, 0, .5);\n}\n\n.left-panel h2 {\n    font-family: 'Castoro', serif;\n    margin-bottom: 1rem;\n    margin: 1rem 4rem;\n    \n}\n\n.ais-RefinementList-list{\n    margin: 1rem 4rem;\n}\n\n.ais-RefinementList-labelText{\n    color: black;\n    font-family: \"Roboto\";\n    font-size: 1rem;\n    font-weight: 300;\n}\n\n.ais-RefinementList-count{\n    width: 20px;\n    height: 20px;\n    font-size: .6rem;\n    font-style: italic;\n    color: white;\n    background-color: rgba(78, 78, 78, 0.295);\n    border-radius: 5px;\n\n}\n\n.ais-RefinementList-checkbox[type=\"color\"] {\n\t-webkit-appearance: none;\n    border: none;\n    border-radius: 50%;\n\twidth: 32px;\n\theight: 32px;\n}\n.ais-RefinementList-checkbox[type=\"color\"]::-webkit-color-swatch-wrapper {\n    padding: 0;\n    border-radius: 50%;\n}\n.ais-RefinementList-checkbox[type=\"color\"]::-webkit-color-swatch {\n    border: none;\n    border-radius: 50%;\n}\n\n@keyframes fadeFilters {\n    0%{\n        transform: translateY(-100%);\n        visibility: visible;\n        opacity: 0.2;\n    }\n    100%{\n        transform: translateY(0%);\n        visibility: visible;\n        opacity: 1;\n    }\n}\n@keyframes fadeOutFilter {\n    0%{\n        transform: translateY(0%);\n        visibility: visible;\n        opacity: 1;\n    }\n    100%{\n        transform: translateY(-100%);\n        visibility: hidden;\n        opacity: 0;\n    }\n}\n\n\n@media screen and (max-width: 768px){\n    .left-panel {\n        grid-template-columns: repeat(1, 1fr);  \n        padding: 0rem;   \n    }\n    .left-panel h2{\n        margin: 1rem 0rem;\n    }\n\n    .ais-RefinementList-list {\n        margin: 1rem 0rem;\n        font-size: 0.6rem;\n    }\n   }"},"lineCount":199}},"error":null,"hash":"d310198a8594931b8538ee72fecddd0e","cacheData":{"env":{}}}