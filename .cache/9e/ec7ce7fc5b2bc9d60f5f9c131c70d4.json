ility: hidden; */\n   font-size: 1rem;\n   font-family: 'Roboto';\n    color: black;\n    font-weight: bold;\n    letter-spacing: 0.3rem;\n}\n\n.fadeInSize{\n    animation: fadeInSize 0.3s forwards;\n}\n\n@media screen and (max-width: 768px){\n    body {\n        overflow-x: hidden !important;\n    }\n    .ais-Hits-list{\n        grid-template-columns: repeat(2, 1fr);\n    }\n   }\n   @media screen and (max-width: 560px){\n    body {\n      overflow-x: hidden !important;\n  }\n  .ais-Hits-list{\n    grid-template-columns: repeat(1, 1fr);\n}\n\n.ais-Stats-text{\n    font-size: 0.8rem;\n}\n.btn-filter {\n    padding: 0.2rem 1rem;\n}\n}\n\n\n/* ANIMATION SIZE HOVER */\n\n@keyframes fadeInSize{\n    0%{\n        visibility: hidden;\n        transform: translateY(4rem) scale(1);\n\n    }\n    100%{\n        visibility: visible;\n        transform: translateY(0rem) scale(1.1);\n    }\n}"},"lineCount":null}},"error":null,"hash":"f0d634d37c8bb2b05c75d5680381ab06","cacheData":{"env":{}}}