\na{\n  text-decoration: none;\n}\n\nbody {\n  padding: 0;\n  margin: 0;\n  background-color: #ffffff;\n  overflow-x: hidden;\n\n}\n\n\n/* .searchbox{\nalign-self: center;\nposition: absolute;\nwidth: 60%;\nright: 5rem;\n} */\n\n/* .ais-SearchBox-input{\n  background-color:rgb(246,245,243);\n  border: none;\n  padding: 0.6rem;\n  padding-left: 2rem;\n  border-radius: 5px;\n} */\n/* \n.ais-SearchBox-input::placeholder {\n  color: grey;\n  font-family: 'Roboto', sans-serif;\n  font-weight: 100;\n  font-style: italic;\n  letter-spacing: .1rem;\n  padding-left: 2rem;\n  font-size: 0.8rem;\n} */\n\n/* CAROUSEL IMAGE GLIDEJS */\n\n.banner-carousel{\n  max-height: 70vh;\n  overflow-y: hidden;\n\n}\n.banner-carousel-img1{\n  width: 100%;\n  object-fit: contain;\n  object-position: 0px -18rem;\n}\n.banner-carousel-img2{\n  width: 100%;\n  object-fit: contain;\n  object-position: 0px -15rem;\n}\n\n\n/* CAROUSEL PRODUCT */\n\n.container {\n  max-width: 90%;\n  margin: 0 auto;\n  padding: 1rem;\n  overflow-x: hidden;\n}\n\n.carousel section {\n\n}\n\n.carousel h2 {\n  margin: 0rem 2rem;\n  color: #161616;\n  position: relative;\n  font-size: 2rem;\n  z-index: auto;\n  align-self: center;\n}\n\n.carousel .carousel-list-container {\n  flex-wrap: nowrap !important;\n  display: -webkit-box !important;\n  overflow-x: scroll !important;\n  padding: 2rem;\n  list-style-type: none;\n}\n\n.title-carousel-winter{\n  margin: 2rem auto;\ndisplay: flex;\njustify-content: space-between;\n}\n\n.btn-carousel-winter{\n  padding: 0.6rem 2rem;\n  border: 1px solid rgba(0, 0, 0, .5);\n  font-size: 1;\n  font-family: \"Roboto\";\n  color: black;\n  align-self: center;\n  cursor: pointer;\n  transition: 0.3s ease-in-out;\n}\n.btn-carousel-winter:hover{\n  background-color: rgb(17, 17, 17);\n  color: white;\n  transition: 0.3s ease-in-out;\n}\n\n/* HORIZONTAL SCROLLBAR */\n\n.carousel .carousel-list-container::-webkit-scrollbar {\n  -webkit-appearance: none;\n}\n\n.carousel .carousel-list-container::-webkit-scrollbar:horizontal {\n  height: 4px;\n\n}\n\n.carousel .carousel-list-container::-webkit-scrollbar-thumb {\n  border-radius: 8px;\n  border: 1px solid white;\n  background-color: rgba(0, 0, 0, .9);\n  \n}\n.carousel .carousel-list-container::-webkit-scrollbar-track { \n  background-color: #fff; \n  border-radius: 8px;\n  \n}\n\n.carousel .carousel-list-container li {\n  min-height: 450px;\n  padding: 1.5rem;\n  margin: auto 1rem;\n  width: 380px;\n  background-color: white;\n  position: relative;\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  border: 1px solid rgba(240,240,239, 0.9);\n  transition: 0.2s ease-in-out;\n\n}\n.carousel .carousel-list-container li:hover {\n\n  box-shadow: 4px 4px 20px rgba(240, 240, 239, 1) ;\n  transition: 0.2s ease-in-out;\n}\n\n\n.carousel .carousel-list-container li .title {\ntext-align: center;\nfont-family: 'Roboto';\nfont-weight: 500;\nfont-size: 0.8rem;\n\n}\n\n.carousel .carousel-list-container .image-wrapper {\n max-width: 90%;\n display: flex;\n justify-content: center;\n margin: 0 auto;\npadding: 1rem;\n \n}\n\n.carousel .carousel-list-container li img {\n  width: 70%;\n  object-fit: contain;\n  align-self: center;\n  margin-top: -6rem;\n  margin-bottom: 1rem;\n  filter: drop-shadow(rgb(240, 240, 239) 32px 60px 20px);\n  transition: 0.2s ease-in-out;\n}\n.carousel .carousel-list-container li img:hover {\ntransform: scale(1.1);\nfilter: drop-shadow(16px 16px 20px rgb(240,240,239));\ntransition: 0.2s ease-in-out;\n}\n\n/* STAT */\n.ais-Stats{\n  font-family: \"Roboto\" ;\n  font-weight: 100;\n  font-style: italic;\n  color: black;\n  float: right;\n}\n\n\n\n\n\n\n"},"lineCount":null}},"error":null,"hash":"18b21942949f3940a498c129a2d5d59b","cacheData":{"env":{}}}